<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <DestinationPath Condition="$(DestinationPath)==''">$(PowerShellPersonalModulePath)</DestinationPath>
    <DestinationPrefix Condition="$(DestinationPrefix)=='' and $(PowerShellModuleName)!=''">$(PowerShellModuleName)\</DestinationPrefix>
    <CommitPrefix Condition="$(CommitPrefix)=='' and $(PowerShellModuleName)!=''">$(PowerShellModuleName)\</CommitPrefix>
  </PropertyGroup>
</Project>
